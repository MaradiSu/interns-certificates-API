// .vscode/launch.json
{
    "version": "0.2.0",
    "configurations": [
        
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Node.js App (Development)",
        "program": "${workspaceFolder}/index.js",
        "runtimeArgs": ["--inspect"],
        "env": {
          "NODE_ENV": "development"
        }
      }
    ]
  }
  